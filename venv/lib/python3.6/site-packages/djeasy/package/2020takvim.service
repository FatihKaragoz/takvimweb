[Unit]
Description=takvimweb
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/home/fatih/PycharmProjects/takvimweb
ExecStart=/home/fatih/PycharmProjects/takvimweb/venv/bin/gunicorn --workers 3 --bind unix:/home/fatih/PycharmProjects/takvimweb/takvimweb.sock takvimweb.wsgi:application
[Install]
WantedBy=multi-user.target